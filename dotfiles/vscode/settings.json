  "chat.checkpoints.showFileChanges": true,
  "chat.customAgentInSubagent.enabled": true,
  "chat.editor.wordWrap": "on",
  "chat.edits2.enabled": true,
  "chat.mcp.discovery.enabled": {
    "claude-desktop": true
  },
  "chat.renderRelatedFiles": true,
  "chat.todoListTool.writeOnly": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.codeLens": true,
  "editor.codeLensFontFamily": "'Iosevka'",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fastScrollSensitivity": 5,
  "editor.fontFamily": "'Monaspace Krypton', Consolas, monospace",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": true
  },
  "editor.scrollBeyondLastLine": true,
  "editor.smoothScrolling": true,
  "editor.wordWrap": "on",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "git.autofetch": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.alternateGptPrompt.enabled": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "text": "Escribe los mensajes de commit en español utilizando el formato de Conventional Commits. Por ejemplo, si estás arreglando un error, el mensaje podría ser 'fix: corregir error en la función de autenticación'. Si estás agregando una nueva característica, el mensaje podría ser 'feat: agregar soporte para múltiples idiomas'. Asegúrate de que el mensaje sea claro y conciso, y que describa adecuadamente los cambios realizados. Que describa los cambios realizados en el código. El mensaje debe ser claro y conciso, reflejando las modificaciones hechas. El mensaje debe comenzar con un verbo en infinitivo"
    }
  ],
  "github.copilot.chat.completionContext.typescript.mode": "on",
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true,
  "github.copilot.chat.localeOverride": "es",
  "github.copilot.chat.newWorkspace.useContext7": true,
  "github.copilot.chat.notebook.enhancedNextEditSuggestions.enabled": true,
  "github.copilot.chat.notebook.followCellExecution.enabled": true,
  "github.copilot.chat.responsesApiReasoningSummary": "detailed",
  "github.copilot.chat.scopeSelection": true,
  "github.copilot.chat.useResponsesApi": true,
  "github.copilot.enable": {
    "*": true,
    "javascript": true,
    "markdown": true,
    "plaintext": false,
    "python": true,
    "scminput": false,
    "typescript": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "jest.autoRun": "watch",
  "npm.scriptExplorerAction": "run",
  "redhat.telemetry.enabled": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "vite.autoStart": false,
  "vite.browserType": "system",
  "vite.open": false,
  "workbench.colorTheme": "Kamekazi Dark (Italic)",
  "workbench.sideBar.location": "right",
  "workbench.tree.indent": 16
}
