  "editor.suggestFontSize": 12,
  "editor.suggestSelection": "recentlyUsed",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editot.guides.highlightActiveIndentation": true,
  "errorLens.enabled": true,
  "errorLens.excludeBySource": [
    "pylint"
  ],
  "errorLens.followCursor": "activeLine",
  "eslint.format.enable": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.experimental.affinity": {
    "github.copilot": 2,
    "ms-python.python": 1,
    "ms-python.vscode-pylance": 1
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "**/templates/**/*.html": "django-html",
    "*.djhtml": "django-html",
    "*.html": "django-html"
  },
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.mypy_cache/**": true,
    "**/.pytest_cache/**": true,
    "**/__pycache__/**": true,
    "**/node_modules/**": true,
    "**/venv/**": true
  },
  "git.autofetch": true,
  "git.experimental.generateCommitMessage.enabled": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.alternateGptPrompt.enabled": true,
  "github.copilot.chat.anthropic.thinking.enabled": true,
  "github.copilot.chat.anthropic.tools.websearch.enabled": true,
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "text": "Escribe los mensajes de commit en español utilizando el formato de Conventional Commits. Por ejemplo, si estás arreglando un error, el mensaje podría ser 'fix: corregir error en la función de autenticación'. Si estás agregando una nueva característica, el mensaje podría ser 'feat: agregar soporte para múltiples idiomas'. Asegúrate de que el mensaje sea claro y conciso, y que describa adecuadamente los cambios realizados. Que describa los cambios realizados en el código. El mensaje debe ser claro y conciso, reflejando las modificaciones hechas. El mensaje debe comenzar con un verbo en infinitivo"
    }
  ],
  "github.copilot.chat.completionContext.typescript.mode": "on",
  "github.copilot.chat.completionsFetcher": "node-fetch",
  "github.copilot.chat.customInstructionsInSystemMessage": true,
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.edits.temporalContext.enabled": true,
  "github.copilot.chat.generateTests.codeLens": true,
